#include <stdio.h>


//------------------------------------------求最大公约数与最小公倍数--------------------------
int main()
{
    int a,b,r;
    scanf("%d %d",&a,&b);
    if(a<b)
    {
        int temp = a;
        a = b;
        b = temp;
    }

    int multiple = a*b;
    while(b!=0)
    {
        r = a%b;
        a = b;
        b = r;
    }

    printf("divisor = %d,mutipule=%d",a,multiple/a);
    return 0;
}
